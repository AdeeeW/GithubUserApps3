<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.consumerapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="com.example.githubuserapps2.READ_DATABASE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icongithubuser"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icongithubuser"
        android:supportsRtl="true"
        android:theme="@style/Theme.GithubUserApps2">
        <activity
            android:name=".activity.SpalshScreen"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity"/>
    </application>

    <queries>
        <package android:name="com.example.githubuserapps2" />
    </queries>
</manifest>